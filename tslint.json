{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**",
            "package.json"
        ]
    },
    "jsRules": {},
    "rules": {
        "no-console": false,
        "semicolon": [
            true,
            "never"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "space-before-function-paren": true,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}